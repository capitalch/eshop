            "tsConfig": "projects/gx-dynamic-form/tsconfig.lib.json",
            "project": "projects/gx-dynamic-form/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/gx-dynamic-form/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
